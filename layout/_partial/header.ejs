<!-- BEGIN TOP BAR -->
<div class="pre-header"style="display: none;">
  <div class="container container-full-w">
    <div class="row">
      <!-- BEGIN TOP BAR LEFT PART -->
      <div class="col-md-6 col-sm-6 col-xs-9 additional-shop-info">
	<ul class="list-unstyled list-inline">
	  <li><i class="fa fa-phone"></i><span><%= theme.phone %></span></li>
	  <li><i class="fa fa-envelope-o"></i><span><%= theme.email %></span></li>
	</ul>
      </div>
      <!-- END TOP BAR LEFT PART -->
      <!-- BEGIN TOP BAR MENU -->
      <div class="col-md-6 col-sm-6 col-xs-3 additional-nav">
	<ul class="list-unstyled list-inline pull-right">
	  <li><a href="/login">Log In</a></li>
	</ul>
      </div>
      <!-- END TOP BAR MENU -->
    </div>
  </div>        
</div>
<!-- END TOP BAR -->
<!-- BEGIN HEADER -->
<div class="header <%= index ? 'is-index' : '' %>">
  <div class="container container-full-w">
    <a class="site-logo" href="<%- url_for() %>" id="logo"><div class="site-logo__image"></div><span class="site-logo__title"><%= config.logo_title %></span></a>


    <a href="javascript:void(0);" class="mobi-toggler"><i class="fa fa-bars"></i></a>

    <!-- BEGIN NAVIGATION -->
    <div class="header-navigation pull-right font-transform-inherit">
      <ul>
	<% for (var i in theme.menu){ %>
	<li class="<%= url_for(theme.menu[i]) == url_for(path).substring(0, url_for(path).lastIndexOf('/') + 1) ? 'active' : ''   %>">
	  <a  href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
	</li>
	<% } %>
	<!-- BEGIN TOP SEARCH -->
	<!-- END TOP SEARCH -->
      </ul>
    </div>
    <!-- END NAVIGATION -->
  </div>
</div>
<!-- Header END -->

