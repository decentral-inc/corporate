&.icon {
  i {

    &[class^='icon-'] {
      display: inline-block;
      height: 1em;
      width: 1em;
      position relative
      vertical-align: bottom;

      &:before {
        content: '';
        position absolute;
        background-repeat: no-repeat;
        background-size: contain;
        width: 100%;
        height: 100%;
        left: 0;
        top: 0;
        background-position: 50%, 50%;
      }
    }

    &.icon {
      &-close-envelope {
        width: 1.4em;
        &:before {
          background-image: url('../images/icons/close_envelope.png');
        }

        &:hover {
          &:before {
            background-image: url('../images/icons/close_envelope_selected.png');
          }
        }
      }
    }
  }
}

