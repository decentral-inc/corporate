
* {
  box-sizing: border-box
}

body * {
  outline none
}

body {
  font-size: 16px
  line-height: 1.35em;
  text-align: center
  font-family Roboto
  color: #222430

  @media mq-mobile {
    font-size: calc((100vw + 1440px) / 160) // 12 - 11
  }
  @media mq-tablet {
    font-size: calc((100vw + 2976px) / 288) // 13 - 12
  }
  @media mq-normal-to-ipad {
    font-size: calc((100vw + 2560px) / 256) // 14 - 13
  }
  @media mq-ipad-to-lg {
    font-size: calc((100vw + 5248px) / 448) // 16 - 14
  }
}

.h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6 {
  line-height: 1.35;
  margin 0
}

.h2, h2 {
  font-size: 40px
  @media mq-mobile {
    font-size: calc((100vw + 400px) / 40) // 22 - 18
  }
  @media mq-tablet {
    font-size: calc((100vw + 1104px) / 72) // 26 - 22
  }
  @media mq-normal-to-ipad {
    font-size: calc((100vw + 341.3333px) / 42.6667) // 32 - 26
    line-height: 1.35em
  }
  @media mq-ipad-to-lg {
    font-size: calc((100vw + 2560px) / 112) // 40 - 32
    line-height: 1.35em
  }
}

.h3, h3 {
  font-size: 38px
  @media mq-mobile {
    font-size: calc((100vw + 320px) / 40) // 20 - 16
  }
  @media mq-tablet {
    font-size: calc((100vw + 960px) / 72) // 24 - 20
  }
  @media mq-normal-to-ipad {
    font-size: calc((100vw + 256px) / 42.6667) // 30 - 24
  }
  @media mq-ipad-to-lg {
    font-size: calc((100vw + 2336px) / 112) // 38 - 30
  }
}

h2 {
  margin-bottom: 52px
  span {
    display: inline-block;
  }
}
.m-col {
  @media mq-mobile {
    flex-wrap: wrap;
  }
}

.row-center {
  display: flex;
  justify-content: center;
}

.row-v-center {
  display: flex;
  align-items: center;
}

.row-flex {
  display: flex;
}

.col-center {
  display: flex;
  flex-direction: column
  align-items: center;
}

.col-left {
  display: flex;
  flex-direction: column
  align-items: flex-start;
}
.col-right {
  display: flex;
  flex-direction: column
  align-items: flex-end;
}

.flex-row {
  display: flex
  align-items: center;
}

.row-100 {
  width 100%
}

.row-right {
  display: flex
  justify-content: flex-end;
}

.f-align-baseline {
  align-items baseline
}
.col-lg-20 {
  width: 20%
}

.col-lg-30 {
  width: 30%
}

.col-lg-70 {
  width: 70%
}

.col-lg-65 {
  width: 65%
}

.col-pos-unset {
  position: unset
}

.col-align-center {
  margin 0 auto
}

.coll-container {
  display flex
  flex-direction column
}

.coll-center {
  display flex
  justify-content center
}

.coll-lg-65 {
  width 65%
}

.coll-lg-70 {
  width 70%
}

  .col-v-center {
    justify-content center
  }
.padding-h-10 {
  padding-left: 10px
  padding-right: 10px
}

.padding-h-60 {
  padding-left: 60px
  padding-right: 60px

  @media mq-normal-to-ipad {
    padding-left: calc(100vw / 17.06667) // 60 - 45
    padding-right: calc(100vw / 17.06667) // 60 - 45
  }
  @media mq-tablet {
    padding-left: calc(100vw / 17.06667) // 45 - 30
    padding-right: calc((100vw + 96px) / 19.2) // 45 - 30
  }
  @media mq-mobile {
    padding-left: 0px
    padding-right: 0px
    width: 100%
  }
}

p {
  font-weight 300

  & > span {
    display: inline-block
  }
}

.corporate {
  font-family: Roboto

  &.index {
    overflow-y: hidden;

    .section-footer {
      height: 100%;
    }
  }

  &:not(.index) {

    .main {
      margin-top 113px

      @media mq-to-normal {
        margin-top 90px
      }
      @media mq-mobile {
        margin-top 80px
      }
    }

    .section-footer {
      .section-inner {
        padding-top 50px
        padding-bottom 50px
      }
    }
  }
}

.section {
  height: 100%
  min-height: 100%
  //min-height: 100vh
  //height: 1080px
  display: block
  width: 100%

  &-inner {
    width: 100%
    height: 100%
  }

  &-content {
    width: 100%
    height: 100%
    display: flex
    align-items: center
    justify-content: center
    padding: 80px 0 65px;
    @mq-normal-to-ipad {
      padding: 60px 10px 55px;
    }
    @media mq-tablet {
      padding: 60px 10px 45px;
    }
    @media mq-mobile {
      padding: 60px 10px 40px;
    }
  }

  &-dark {
    color: #fff
  }
}

.container-full-w {
  width: 100%
  padding: 0 65px

  @media mq-normal-to-ipad {
    padding: 0 30px;
  }
  @media mq-tablet {
    padding: 0 30px;
  }
  @media mq-mobile {
    padding: 0 30px;
  }
  @media mq-mobile-small {
    padding: 0 20px;
  }
}

.btn {
  border: 2px solid #42a3f6
  height 40px
  color: #42a3f6
  font-size 14px
  font-weight 400
  background-color transparent
  padding: 8px 36px;
  border-radius: 20px !important;
  outline none
  @media mq-mobile {
    padding: 7px 32px;
    height 38px
  }
  @media mq-mobile-iphonex {
    padding: 6px 32px;
    height 36px
  }
}



.header {
  position: fixed
  top: 0
  left: 0
  width: 100%
  z-index: 99999
  margin-bottom: 0
  background-color: rgba(255, 255, 255, 0.9)
  box-shadow: none

  &.is-index {
    background-color: transparent
  }
}

.header .site-logo,
.header-navigation > ul > li > a
  color: color-grey
  transition: 0.5s ease;

.header.dark .site-logo,
.header.dark .header-navigation > ul > li > a {
  color: #fff

  &:hover {
    color: #64aed9
  }
}


.header-navigation > ul > li > a {
  font-size 15px
  text-decoration none
  text-transform uppercase
  padding: 29px 13px 30px;
  letter-spacing: 0.1em;
  font-family Roboto;
}
.header-navigation > ul > li + li {
  margin-left: 15px;
  @media mq-mobile-iphonex {
    margin-left: 5px;
  }
}

.main
  position: top
  min-height 105px
  &.onepage-wrapper
    height: 100vh;

.site-logo {
  display: flex;
  align-items: center;
  font-size 35px
  font-family Roboto-black
  margin-left 10px
  padding-top: 23px;
  padding-bottom 0
  @media mq-normal-to-ipad {
    font-size 30px
  }
  @media mq-tablet {
    margin-right: 0px
    font-size 24px
  }
  @media mq-mobile {
    font-size 22px
    margin-right: 0px
    padding-top: 18px;
    padding-bottom 0
    margin-left: 0
  }
  @media mq-mobile-small {
    font-size 20px
  }

  &:hover {
    text-decoration none
  }

  &__title {
    line-height: 1;
    @media mq-to-normal {
    }
    @media mq-mobile {
    }
  }

  &__image {
    background-image url('../images/logo.png')
    width 60px
    height 60px
    background-repeat no-repeat
    background-size contain
    margin-right 20px
    transition: 0.5s ease;
    @media mq-normal-to-ipad {
      width 45px
      height 45px
    }
    @media mq-tablet {
      width 40px
      height 40px
    }
    @media mq-mobile {
      width 35px
      height 35px
      margin-right 10px
    }
  }
}

.header {
  &.dark {
    .site-logo__image {
      background-image url('../images/logo-dark.png')
    }
  }
  .header-navigation {
    display block !important
    margin-right 0

    @media mq-mobile {
      padding: 0;
      margin: 0;
    }
  }
  .mobi-toggler {
    display none
  }

  .container-full-w {
    @media mq-mobile-iphonex {
      padding: 0 25px;
    }
  }
}


.section {
  background-repeat: no-repeat;
  background-size: cover;
  background-position: 50%, 50%;
  padding-left: 0;
  padding-right: 0;
  overflow hidden

  &-inner {
    padding-left: 15px;
    padding-right: 15px;
    position: relative
  }

  .social-wrapper {
    position absolute
    bottom: 35px
    left: 50%;
    transform: translateX(-50%);
    font-size: 28px;

    @media mq-mobile {
      bottom: 10px
    }
    @media mq-tablet {
      bottom: 15px
    }
    @media mq-normal-to-ipad {
      bottom: 25px
    }

    a {
      color: #484a57;
    }

    .social-footer {
      margin-bottom 0
      display: flex;
    }

    .list-inline>li {
      padding-left: 8px;
      padding-right: 8px;
    }
  }

  &-dark {
    .social-wrapper {
      a {
        color: #fff;
      }
    }
  }
}

.section1 {
  background-color: color-bg-grey
  background-image url(../images/section_1.png)
  font-size: 24px
  @media mq-mobile {
    font-size: calc((100vw + 640px) / 80) // 14 - 12
  }
  @media mq-tablet {
    font-size: calc((100vw + 1536px) / 144) // 16 - 14
  }
  @media mq-normal-to-ipad {
    font-size: calc((100vw + 256px) / 64) // 20 - 16
  }
  @media mq-ipad-to-lg {
    font-size: calc((100vw + 3456px) / 224) // 24 - 20
  }

  .col-lg-30,
  .col-lg-20 {
    @media mq-tablet {
      width: 30%
    }
    @media mq-normal-to-ipad {
      width: 30%
    }

    &.m-100 {
      @media mq-mobile {
        width 100%
      }
    }
  }

  .string {
    @media mq-mobile {
      display: inline;
    }
  }

  h2 {
    text-align: left;
    font-size 38px
    line-height: 1.3;
    @media mq-ipad {
      line-height: 1.1;
    }
    @media mq-tablet {
      line-height: 0.75;
    }
    @media mq-mobile {
      line-height: 1;
      margin-bottom: 10px
      text-align: center
    }

    span {
      line-height 1

      @media mq-sn-to-lg {
        font-size: 0.9em;
      }
      @media mq-ipad-to-sn {
        font-size: 0.75em;
      }
      @media mq-normal-with-ipad {
        font-size: 0.65em;
      }
      @media mq-tablet {
        font-size: 0.38em;
      }
      @media mq-mobile {
        font-size: 0.65em;
      }
    }
  }

  .logo-wrapper {

    @media mq-normal-h-600 {
      max-width: calc(100vh - 80px - 65px);
      margin-left: auto;
    }
  }

  .section-logo {
    background-image url('../images/logo_big.png')
    background-repeat no-repeat
    background-size contain
    width: 100%;
    height: 100%;
    padding-bottom: 100%;
    margin-left: auto;
    max-width 400px
    margin-right 5vw
    background-position-y: 40%;
    width: 19.79vw;

    @media mq-sn-to-lg {
      min-width 19.79vw
    }
    @media mq-ipad-to-sn {
      min-width 19.79vw
    }
