/* articles */

.articles {
  //width 100%
  display flex
  margin-left: -30px;
  margin-right: -30px;
  padding-bottom: 50px;
  flex-wrap: wrap;
  @media mq-mobile {
    margin-left: -15px;
    margin-right: -15px;
  }
  @media mq-tablet {
    margin-left: -15px;
    margin-right: -15px;
  }
  @media mq-normal-with-ipad {
    margin-left: -15px;
    margin-right: -15px;
  }
  @media mq-from-ipad-to-lg {
    margin-left: -30px;
    margin-right: -30px;
  }

  .article {
    width 33.333%
    padding: 30px 30px 60px 30px;
    box-sizing border-box
    @media mq-mobile {
      width 100%
      padding: 15px 15px 30px 15px;
    }
    @media mq-tablet {
      width 50%
      padding: 15px 15px 30px 15px;
    }
    @media mq-normal-with-ipad {
      width 50%
      padding: 15px 15px 30px 15px;
    }
    @media mq-from-ipad-to-lg {
      width 33.333%
      padding: 30px 30px 60px 30px;
    }

    &-inner {
      width 100%
      padding-bottom 100%
      height 0
      position: relative;
    }

    &-bg {
      position: absolute
      width: 100%;
      height: 100%;
      top: 0;
      overflow: hidden;

      &-img {
        background-repeat: no-repeat;
        background-size: cover;
        width: 100%;
        height: 100%;
        background-position: 50% 50%;
      }

      &-link {
        width 100%;
        height 100%;
        position absolute
        left: 0;
        top: 0;
        z-index 1
      }
    }

    &-content {
      position: absolute
      width: 100%;
      height: 100%;
      top: 0;
      padding: 35px 25px 40px;
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      background-color: rgba(0, 0, 0, 0.3);
      @media mq-mobile {
        padding: 15px;
      }
      @media mq-tablet {
        padding: 20px;
      }
      @media mq-normal-with-ipad {
        padding: 25px;
      }
      @media mq-from-ipad-to-lg {
        padding: 35px 25px 40px;
      }

      &-top {
        //flex-grow 1
        z-index 2
        display: flex;
        align-items: flex-start;
        justify-content: left;
      }

      &-bottom {
        //flex-grow 1
        z-index 2
        display: flex;
        align-items: flex-end;
        justify-content: center;
      }
    }

    &-title {
      color color-text-white;
      font-size 32px
      font-weight bold
      line-height: 1.35em;
      text-decoration none;
      @media mq-mobile {
        font-size: 16px
      }
      @media mq-tablet {
        font-size: 20px
      }
      @media mq-normal-with-ipad {
        font-size: 24px
      }
      @media mq-from-ipad-to-lg {
        font-size: 28px
      }

      &:hover {
        text-decoration none;
        color: color-text-selected;
      }
    }

    &-bottom {
      margin-top: 20px;
      border-top: 1px solid #dadada
      padding-top 20px;

      &-inner {
        display: flex;
        justify-content: space-between;
      }

      &-left {
        display: flex;
        align-items: center;
      }

      &-right {

      }
    }


    .tags-list {
      .tag {
        margin-right 10px
        margin-bottom 10px
      }
    }
  }
}

