  p {
    text-align left

    @media mq-sn-to-lg {
      font-size: 0.85em;
    }
    @media mq-ipad-to-sn {
      font-size: 0.8em;
    }
    @media mq-normal-to-ipad {
      font-size: 0.8em;
    }
    @media mq-tablet {
      font-size: 0.9em;
    }
    @media mq-mobile {
      font-size: 1em;
      text-align: center
    }
  }
  .m-col {
    @media mq-mobile {
      flex-direction: column;
    }
  }

  &-custom-col {
    left: -1.75vw;
    top: 1.75vw;
    @media mq-mobile {
      left: 0;
      top: 0;
    }

    & > .row {
      @media mq-mobile {
        display flex
        flex-wrap: wrap
      }
    }
  }

  .row-custom {
    transform: translateY(4.5vh);
    @media mq-mobile {
      transform: translateY(1.5vh);
    }
  }

  .col-1 {
    width: calc(30 / 120 * 100%);
    @media mq-mobile {
      width: 100%
    }
  }
  .col-2 {
    width calc(42 / 120 * 100%)
    @media mq-mobile {
      width: 100%
    }

    p {
      transform: translateX(15%);
      @media mq-ipad {
        transform: none;
        text-align: center;
      }
    }
  }
  .col-3 {
    width calc(48 / 120 * 100%)
    @media mq-mobile {
      width: 100%
    }
  }

  .col-logo {
    @media mq-ipad-to-1200 {
      width: 35%
    }
    @media mq-ipad {
      width: 25%
      align-self: flex-start;
    }
    @media mq-mobile {
      width: 50%;
      margin: 0 auto 15px;
    }
  }

  .col-text {
    @media mq-ipad-to-1200 {
      width: 65%
    }
    @media mq-ipad {
      width: 75%
    }
    @media mq-mobile {
      width: 100%
    }
  }
}
.section2 {
  background-color: color-bg-white

  .section-content > .col-lg-12 {
    @media mq-ipad-v {
      height: 100%;
    }
  }

  .boxes {
    @media mq-ipad {
      display flex
      flex-wrap: wrap
    }
    @media mq-ipad-h {

    }
    @media mq-to-normal {
      flex-direction: column
      align-items: center;
    }
    @media mq-ipad-h {
      flex-direction: row
    }
    @media mq-ipad-v {
      height: 100%;
    }
  }

  .box {
    background-repeat: no-repeat;
    background-size: cover;
    background-position: 50%, 50%;
    color: #fff;
    font-size: 25px // 25px
    @media mq-to-normal {
      display flex
      flex-wrap: wrap
    }
    @media mq-mobile {
      font-size: calc((100vw + 640px) / 80) // 14 - 12
    }
    @media mq-tablet {
      font-size: calc((100vw + 1536px) / 144) // 16 - 14
    }
    @media mq-normal-to-ipad {
      font-size: calc((100vw + 256px) / 64) // 20 - 16
    }
    @media mq-ipad-to-lg {
      font-size: calc((100vw + 2560px) / 179.2) // 25 - 20
    }

    &.square {
      width: 543px
      height: 543px
      @media mq-screen-lg {
        width: 28.28vw
        height: 28.28vw
      }
      @media mq-ipad  {
        width: calc(50% - 1.432vw)
        height: 24vw
      }
      @media mq-ipad-v  {
        width: calc(33% - (2.864vw) * 2 / 3)
        height: 50vh;
      }
      @media mq-ipad-h {
      }
      @media mq-to-normal {
        width: 90%
        height: 24vh
        min-height 160px
      }
      @media mq-to-normal-v {
        min-height auto
      }
      @media mq-to-normal-h {
        width: calc(50% - (2.864vw) * 2 / 3)
        height: calc((100vh - 80px - 60px - 20px) * 0.5 )
      }
      @media mq-normal-h-600 {
        max-width: calc(100vh - 80px - 65px - 20px);
        max-height: calc(100vh - 80px - 65px - 20px);
      }
      @media mq-mobile {
        width: 90%
        height: calc(33% - (2.864vw) * 2 / 3)
        min-height 130px
      }
      @media mq-mobile-h {
        width: calc(33% - (2.864vw) * 2 / 3)
        height: 50vh;
      }
      @media mq-mobile-v {
        min-height auto
      }
    }

    &.box-1 {
      background-image: url(../images/box_1_bg.png)
    }
    &.box-2 {
      background-image: url(../images/box_2_bg.png)
    }
    &.box-3 {
      background-image: url(../images/box_3_bg.png)
      @media mq-ipad {
        margin-top: 2.864vw
      }
    }

    &-inner {
      width: 100%;
      height: 100%;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      padding: 30px;
      @media mq-ipad-h {
        justify-content: flex-start;
      }
      @media mq-normal-to-ipad {
        padding: 20px;
      }
      @media mq-to-normal-h {
        padding: 15px;
      }
      @media mq-ipad-v and (min-width: 480px) {
        padding-top 15px
        padding-bottom 15px
      }
      @media mq-screen-sn {
        justify-content: flex-start;
      }

      p {
        min-height 3em
        width: 90%;
        margin-bottom 0
        @media mq-mobile {
          font-size: 14px
        }
        @media mq-tablet {
          font-size: 16px
        }
        @media mq-ipad-h {
          font-size: 14px
        }
        @media mq-ipad-v {
          min-height 1em
        }

        span {
          display initial
        }
      }
    }

    & + .box {
      margin-left 55px
      @media mq-screen-lg {
        margin-left: 2.864vw
      }
      @media mq-to-normal {
        margin-left 0
        margin-top: 2.864vw
      }
      @media mq-to-normal-h {
        margin-left: 2.864vw
        margin-top: 0
      }

      &.box-3 {
        @media mq-ipad {
          margin-left 0
        }
        @media mq-ipad-h {
          margin-left: 2.864vw
        }
        @media mq-ipad-h {
          margin-top: 10px
        }
      }
    }

    h3 {
      color: #42a3f6
      margin-bottom 37px
      font-family Roboto-black
      @media mq-mobile {
        margin-bottom 15px
        font-size: 24px
      }
      @media mq-tablet {
        margin-bottom: calc((100vw - 48px) / 28.8) // 25 - 15
        font-size: 24px
      }
      @media mq-normal-to-ipad {
        margin-bottom: calc((100vw - 234.6667px) / 21.3333) // 37 - 25
      }
      @media mq-ipad {
        font-size: 18px
        margin-bottom 25px
        display: flex;
        align-items: flex-end;
      }
      @media mq-ipad-h {
        font-size: 18px
        margin-bottom 6.5%
        min-height: 2.7em;
        height: 33%;
      }
      @media mq-ipad-v and (min-width: 480px) {
        margin-bottom 15px
      }
      @media mq-screen-sn {
        min-height: calc(1.35em * 2);
        margin-top: 1em;
      }
      @media screen and (min-width: 480px) and (max-height: 700px) {
        margin-top: 0.25em;
        min-height: calc(1.35em * 1.25);
      }
      @media mq-to-normal-v {
        margin-top: 0;
      }
      @media mq-ipad-h {
        margin-top: 0;
        height: calc(50% - 20px);
      }
    }
  }
}
